{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "env": {
        "SSH_PRIVATE_KEY": "@ssh_private_key"
      }
    }
  ],
  "routes": [
    {
      "src": "/graphql",
      "dest": "server/index.js"
    },
    {
      "src": "/health",
      "dest": "server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ]
}